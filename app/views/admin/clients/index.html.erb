<p id="notice"><%= notice %></p>

<h1>Clients</h1>

<table>
  <thead>
    <tr>
      <th>Fname</th>
      <th>Mname</th>
      <th>Lname</th>
      <th>Contact</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @clients.each do |client| %>
      <tr>
        <td><%= client.fname %></td>
        <td><%= client.mname %></td>
        <td><%= client.lname %></td>
        <td><%= client.contact %></td>

      </tr>
    <% end %>
  </tbody>
</table>
